// Online C++ compiler to run C++ program online
#include <bits/stdc++.h>
using namespace std;

int main() {
    char s[100];
    int test,i;
    cin>>test;

    for(i=0;i<test;i++)
    {
        cin>>s;
        int a1=int(s[0]);
        a1=a1-48;
        int b1=int(s[1]);
        b1=b1-48;
        int c1=int(s[2]);
        c1=c1-48;
        int d1=int(s[3]);
        d1=d1-48;
        int e1=int(s[4]);
        e1=e1-48;
        int f1=int(s[5]);
        f1=f1-48;

        if(a1+b1+c1==d1+e1+f1)
        {
            cout<<"YES"<<endl;
        }
        else
        {
            cout<<"NO"<<endl;
        }
    }

    return 0;
}
